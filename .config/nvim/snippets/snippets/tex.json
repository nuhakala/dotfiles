{
    "table": {
        "prefix": "table",
        "body": [
            "\\begin{table}[h]",
            "\t\\centering",
            "\t\\begin{tabular}{$1}",
            "\t\t% Header row",
            "\t\t${4:header row} \\\\\\",
            "\t\t\\hline",
            "\t\t% Content",
            "\t\t${5:content}",
            "\t\\end{tabular}",
            "\t\\caption{$2}",
            "\t\\label{table:$3}",
            "\\end{table}",
            "$0"
        ],
        "description": "Figure template"
    },
    "figure": {
        "prefix": "figure",
        "body": [
            "\\begin{figure}[h]",
            "\t\\begin{center}",
            "\t\t\\includegraphics[width=0.95\\textwidth]{$1}",
            "\t\\end{center}",
            "\t\\caption{$3}",
            "\t\\label{fig:$4}",
            "\\end{figure}",
            "$0"
        ],
        "description": "Figure template"
    },
    "Itemize": {
        "prefix": "list",
        "body": ["\\begin{itemize}", "\t\\item $0", "\\end{itemize}"],
        "description": "Itemize"
    },
    "Enumerate": {
        "prefix": "numberedlist",
        "body": ["\\begin{enumerate}", "\t\\item $0", "\\end{enumerate}"],
        "description": "Itemize"
    }
}
