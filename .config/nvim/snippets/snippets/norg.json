{
    "code": {
        "prefix": "code",
        "body": ["@code ${1:lang}", "$0", "@end"],
        "description": "code block"
    },
    "data": {
        "prefix": "data",
        "body": ["@data", "$0", "@end"],
        "description": "data tags"
    },
    "math": {
        "prefix": "math",
        "body": ["@math", "$0", "@end"],
        "description": "math block"
    },
    "table": {
        "prefix": "table",
        "body": ["@table", "$0", "@end"],
        "description": "table"
    },
    "link": {
        "prefix": "link",
        "body": ["{${1:object}}[${2:description}]"],
        "description": "links"
    },
    "task": {
        "prefix": "task",
        "body": ["- ( ) ${1:task}"],
        "description": "task"
    },
    "typst": {
        "prefix": "typst",
        "body": ["@typst", "$0", "@end"],
        "description": "typst environment"
    },
    "Typst algorithm": {
        "prefix": "#algo",
        "body": ["#algo(fill: black, title: \"${1:name}\", parameters: (\"${2:params}\",), indent-guides: 1pt + gray)[", "$0", "]"],
        "description": "Typst algorithm environment"
    },
    "slink": {
        "prefix": "slink",
        "body": ["{${1:object}}[${2:description}]${0} (accessed $CURRENT_DATE.$CURRENT_MONTH.$CURRENT_YEAR)" ],
        "description": "Source link with accessed data"
    },
    "linkfile": {
        "prefix": "linkfile",
        "body": ["{:./${1:Link}:}[${2:Description]"],
        "description": "Workspace link"
    },
    "quellen": {
        "prefix": "quellen",
        "body": ["*Quellen:*", "- $0"],
        "description": "Quellen"
    },
    "sources": {
        "prefix": "sources",
        "body": ["*Sources:*", "- $0"],
        "description": "Sources"
    },
    "l채hteet": {
        "prefix": "l채hteet",
        "body": ["*L채hteet:*", "- $0"],
        "description": "L채hteet"
    }
}
